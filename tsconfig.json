{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@assets": ["src/assets"],
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@hooks/*": ["src/hooks/*"],
      "@hooks": ["src/hooks"],
      "@i18n/*": ["src/i18n/*"],
      "@i18n": ["src/i18n"],
      "@modules/*": ["src/modules/*"],
      "@modules": ["src/modules"],
      "@navigation/*": ["src/navigation/*"],
      "@navigation": ["src/navigation"],
      "@utils/*": ["src/utils/*"],
      "@utils": ["src/utils"]
    }
  }
}
